{
  "name": "tmembers",
  "version": "1.0.2",
  "description": "Simple script to get the number of channel subscribers in the Telegram",
  "main": "build/index.js",
  "scripts": {
    "ci": "yarn s lint",
    "s": "yarn soda"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorkamyshev/tmembers.git"
  },
  "keywords": [
    "telegram"
  ],
  "author": "Igor Kamyshev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/igorkamyshev/tmembers/issues"
  },
  "homepage": "https://github.com/igorkamyshev/tmembers#readme",
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@solid-soda/scripts": "^1.0.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn soda lint"
    ],
    "*.{js,jsx,ts,tsx,css,json}": [
      "yarn soda pretty",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
